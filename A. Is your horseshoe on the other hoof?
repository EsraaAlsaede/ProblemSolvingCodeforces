#include <iostream>
using namespace std;
int main(){
    int n=4;
    int arr [n];
    int counter=0;
    for (int i = 0; i < n; i++) {
        std::cin >> arr[i];
    }
    for (int i = 0; i < n; i++) {
        for(int j=i+1;j<n;j++){
            if(arr[i]==arr[j]){
            counter++;
            break;
            } 
        }
    }
    std::cout << counter << std::endl;
    return 0;
}
